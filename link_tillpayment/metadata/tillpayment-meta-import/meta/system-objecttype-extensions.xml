                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">OFF</display>
                        <value>OFF</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">OPTIONAL</display>
