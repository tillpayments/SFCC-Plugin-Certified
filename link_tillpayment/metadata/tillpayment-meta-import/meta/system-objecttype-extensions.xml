    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
		    <attribute-definition attribute-id="enableTillpayment">
                <display-name xml:lang="x-default">Enable Tillpayment</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
			<attribute-definition attribute-id="tillPaymentMode">
                <display-name xml:lang="x-default">Payment Mode</display-name>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">Preauthorize</display>
                        <value>preauth</value>
                    </value-definition>
                    <value-definition>
